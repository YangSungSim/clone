def ret_wind3():
    return app.connect(title='STS Starter').Dialog

dlg = timings.wait_until_passes(20, 0.5, ret_wind3)
print('done dlg')

pass_edit = dlg.Edit2
pass_edit.set_focus()
pass_edit.type_keys('**************')
time.sleep(2)

cert_edit = dlg.Edit3
cert_edit.set_focus()
cert_edit.type_keys('**************')

pyautogui.click(x=891,y=591)
time.sleep(10)
pyautogui.click(x=1026,y=669)

time.sleep(35)

pyautogui.hotkey('ctrl','o')

time.sleep(15)


