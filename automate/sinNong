from pywinauto import application
from pywinauto import timings
import pyautogui
import time
import easygui
import webbrowser

##################신한아이#####################

app = application.Application(backend='win32')
app.start(r'주소\gmshstarter.exe')

#pyautogui.typewrite('\n',interval=0.1)

def ret_wind56():
    return app.connect(title='Goodi Starter').Dialog

dlg = timings.wait_until_passes(10,0.5, ret_wind56)

time.sleep(3)
press_edit = dlg.Edit1
press_edit.set_focus()
press_edit.type_keys('*******')

time.sleep(2)
pass_edit = dlg.Edit2
pass_edit.set_focus()
pass_edit.type_keys('*******')
time.sleep(3)

cert_edit = dlg.Edit3
cert_edit.set_focus()
cert_edit.type_keys('**************')
time.sleep(2)

pyautogui.click(x=1235,y=458)

time.sleep(45)


